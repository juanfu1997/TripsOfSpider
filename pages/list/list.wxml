<view class="wrapper">
  <view class="list" wx:for="{{listArray}}" wx:key="id">
    <view class="listContainer" bindtap="goResult" data-id="{{item.id}}">
      <view class="sub_title">日期: {{item.date}}</view>
      <view class="address firstAddress"><text>{{item.name}}</text></view>
      <view class="address"><text>({{item.address}})</text></view>
      <image class="arrow" src="../../images/right-arrow.png" mode="widthFix"></image>
    </view>
  </view>
</view>
<view class="footer">
	<view class="button"><view class="book" bindtap="book"><text>脚印地图</text></view></view>
</view>